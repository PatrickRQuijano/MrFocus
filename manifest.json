{
    "manifest_version": 3,
    "name": "Mr.Focus",
    "version": "0.0.1",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["Content.js"]
        }
    ],
    "icons": { "48": "ghost.png"},
    "action": {
        "default_popup": "popup.html"
      },
    "permissions": [
        "alarms"
    ],
    "background": {
        "service_worker": "event.js"
    },
    "description": "An easy to use extension to help you stay focused on productivity by blocking you out of the most popular entertainment websites:)"
}